ifeq ($(WITH_BOOST),YES)
  ifeq ($(BOOST_EXTERNAL),NO)
    PROD_SYS_LIBS += boost_system
  else
    ifdef BOOST_LIB
      BOOST_DIR        = $(BOOST_LIB)
      PROD_LIBS     += boost
    else
      PROD_SYS_LIBS += boost_system
    endif
  endif
endif
