ADUVC=$(AREA_DETECTOR)/ADUVC
ADCOMPVISION=$(AREA_DETECTOR)/ADCompVision
ADPLUGINBAR=$(AREA_DETECTOR)/ADPluginBar
ADPLUGINEDGE=$(AREA_DETECTOR)/ADPluginEdge

